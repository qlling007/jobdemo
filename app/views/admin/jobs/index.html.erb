<div class="pull-right">
  <%= link_to("add a job", new_admin_job_pathm :class => "btn btn-default") %>
</div>
<br>
<table class="table table-boldered">
  <tr>
    <td>
      <%= link_to(job.title, admin_job_path(job)) %>
    </td>
    <td>
      <%= link_to("edit", edit_admin_job_path(job))%>
